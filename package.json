{
  "name": "string-dedent",
  "version": "2.0.0",
  "description": "De-indents (dedents) passed in strings",
  "keywords": [
    "string",
    "dedent"
  ],
  "main": "dist/dedent.umd.js",
  "module": "dist/dedent.mjs",
  "typings": "dist/types/dedent.d.ts",
  "files": [
    "dist"
  ],
  "author": "Justin Ridgewell <justin@ridgewell.name>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jridgewell/string-dedent.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.12.0"
  },
  "scripts": {
    "lint:ts": "npm run test:lint:ts -- --fix",
    "lint:prettier": "npm run test:lint:prettier -- --write",
    "lint": "run-s -n lint:*",
    "prebuild": "rm -rf dist",
    "build:ts": "tsc --module commonjs",
    "build:rollup": "rollup -c rollup.config.js",
    "build": "run-s -n build:*",
    "test": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:watch": "jest --coverage --watch",
    "test:lint:ts": "tslint  --project tsconfig.json -t codeFrame '{src,test}/**/*.ts'",
    "test:lint:prettier": "prettier --check '{src,test}/**/*.ts'",
    "test:lint": "run-s -n test:lint:*",
    "test:prod": "run-s -n test:lint 'test --no-cache'",
    "preversion": "run-s test:prod build",
    "prepublishOnly": "npm run preversion"
  },
  "devDependencies": {
    "@types/jest": "26.0.24",
    "@types/node": "14.11.1",
    "@types/type-detect": "4.0.1",
    "jest": "26.4.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "rollup": "2.27.1",
    "@rollup/plugin-typescript": "6.0.0",
    "ts-jest": "26.3.0",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "9.0.0",
    "typescript": "4.0.3"
  },
  "dependencies": {}
}
